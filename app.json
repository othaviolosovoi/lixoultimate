{
  "expo": {
    "name": "lixo_ultimate",
    "slug": "lixo_ultimate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_no_bg.png",
    "scheme": "lixoultimate",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Permitir $(PRODUCT_NAME) a acessar suas fotos?",
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location as a fallback for images without GPS data."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_no_bg.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lixoultimate.app",
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "ACCESS_MEDIA_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo_no_bg.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo_no_bg.png",
          "resizeMode": "cover",
          "backgroundColor": "#ffffff",
          "enableFullScreenImage_legacy": true
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Nunito-Black.ttf",
            "./assets/fonts/Nunito-Bold.ttf",
            "./assets/fonts/Nunito-Light.ttf",
            "./assets/fonts/Nunito-Medium.ttf",
            "./assets/fonts/Nunito-Regular.ttf",
            "./assets/fonts/Poppins-Black.ttf",
            "./assets/fonts/Poppins-Bold.ttf",
            "./assets/fonts/Poppins-Regular.ttf"
          ]
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Permitir $(PRODUCT_NAME) a acessar sua c√¢mera?."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Permitir $(PRODUCT_NAME) a acessar suas fotos?"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d3d44083-4729-4c4f-a1d9-3f135adaad2f"
      }
    }
  }
}
